{% extends 'base.html.twig' %}

{% block title %}All programs{% endblock %}

{% block body %}
    <h1>Toutes les séries de la table program : </h1>
    {% for category in categories %}
        <div>
            <a href="{{ path("category_show", { 'categoryName': category.name }) }}">
               <h2>{{ loop.index }} / {{ category.name }}</h2>
            </a>
        </div>
    {% endfor %}

    <a href="{{ path('Home') }}">
        Retour à l'accueil
    </a>
{% endblock %}